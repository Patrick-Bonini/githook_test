<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Search Bar skin"
  ComponentType="SearchBar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Search Submit Button (hover)">
        <Properties>
          <Property
            Name="BackgroundImage">url('SearchIconWhite.png')</Property>
          <Property
            Name="BackgroundColor">#37474F</Property>
          <Property
            Name="BackgroundGradient">vertical #4a5c65 #4a5c65</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button">
        <Properties>
          <Property
            Name="BackgroundImage">url('SearchIconWhite.png')</Property>
          <Property
            Name="BackgroundGradient">vertical #37474F #37474F</Property>
          <Property
            Name="BackgroundColor">#37474F</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Bar">
        <Properties>
          <Property
            Name="BorderTopLeftRadius">5px</Property>
          <Property
            Name="BorderTopRightRadius">5px</Property>
          <Property
            Name="BorderBottomRightRadius">5px</Property>
          <Property
            Name="BorderBottomLeftRadius">5px</Property>
          <Property
            Name="Width">100%</Property>
          <Property
            Name="MarginTop">10px</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Input">
        <Properties>
          <Property
            Name="Placeholder">Search the Docs</Property>
          <Property
            Name="FontStyle">normal</Property>
          <Property
            Name="FontSize">1.1em</Property>
          <Property
            Name="PaddingLeft">10px</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="SearchIconGreen.png"></Resource>
      <Resource
        Name="SearchIconWhite.png"></Resource>
      <Resource
        Name="SearchIconGreen_1.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Search Submit Button (hover)">
            <Properties>
              <Property
                Name="BackgroundColor">#00C895</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Submit Button">
            <Properties>
              <Property
                Name="BackgroundColor">#dcdcdc</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Bar">
            <Properties>
              <Property
                Name="BorderTopLeftRadius">5px</Property>
              <Property
                Name="BorderTopRightRadius">5px</Property>
              <Property
                Name="BorderBottomRightRadius">5px</Property>
              <Property
                Name="BorderBottomLeftRadius">5px</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Input">
            <Properties>
              <Property
                Name="PaddingLeft">15px</Property>
              <Property
                Name="FontSize">1em</Property>
              <Property
                Name="FontStyle">italic</Property>
              <Property
                Name="Placeholder">Search the docs</Property>
            </Properties>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>