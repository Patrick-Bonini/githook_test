<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="1"
  SkinType="Component"
  Comment="HTML5 Search Bar skin"
  ComponentType="SearchBar"
  EnableResponsiveOutput="True"
  conditions="Default.HideFromAllOutput">
  <Stylesheet>
    <Styles>
      <Style
        Name="Search Input">
        <Properties>
          <Property
            Name="BackgroundColor">#d9d9d9</Property>
          <Property
            Name="FontSize">1em</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button">
        <Properties>
          <Property
            Name="BackgroundColor">#dcdcdc</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Bar">
        <Properties>
          <Property
            Name="BorderTopLeftRadius">5px</Property>
          <Property
            Name="BorderTopRightRadius">5px</Property>
          <Property
            Name="BorderBottomRightRadius">5px</Property>
          <Property
            Name="BorderBottomLeftRadius">5px</Property>
          <Property
            Name="BorderTop">solid 1px #cacacb</Property>
          <Property
            Name="BorderRight">solid 1px #cacacb</Property>
          <Property
            Name="BorderBottom">solid 1px #cacacb</Property>
          <Property
            Name="BorderLeft">solid 1px #cacacb</Property>
          <Property
            Name="MarginBottom">10px</Property>
          <Property
            Name="Height">2.5em</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Filter">
        <Properties>
          <Property
            Name="BorderRight">solid 1px #cacacb</Property>
          <Property
            Name="BorderLeft">solid 1px #cacacb</Property>
          <Property
            Name="BorderTop">solid 1px #cacacb</Property>
          <Property
            Name="BorderBottom">solid 1px #cacacb</Property>
          <Property
            Name="Width">30px</Property>
          <Property
            Name="BackgroundColor">#d9d9d9</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Filter Drop-down">
        <Properties>
          <Property
            Name="BackgroundColor">#616161</Property>
          <Property
            Name="Color">#ffffff</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Filter Drop-down (hover)">
        <Properties>
          <Property
            Name="Color">#ffffff</Property>
          <Property
            Name="BackgroundColor">#00C895</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button (hover)">
        <Properties>
          <Property
            Name="BackgroundColor">#00C895</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="skin_search_filter.png"></Resource>
      <Resource
        Name="skin_search_filter.gif"></Resource>
    </Resources>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Search Input">
            <Properties>
              <Property
                Name="FontFamily">Helvetica</Property>
              <Property
                Name="FontSize">12pt</Property>
              <Property
                Name="Placeholder">Search Documentation</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Submit Button">
            <Properties>
              <Property
                Name="BackgroundColor">#daa520</Property>
              <Property
                Name="Tooltip">Search</Property>
            </Properties>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>