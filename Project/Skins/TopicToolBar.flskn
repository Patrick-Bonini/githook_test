  </TopicToolbar>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Toolbar">
            <Properties>
              <Property
                Name="Float">right</Property>
              <Property
                Name="MarginTop">6px</Property>
              <Property
                Name="MarginBottom">30px</Property>
              <Property
                Name="MarginRight">0px</Property>
            </Properties>
          </Style>
          <Medium
            Name="Mobile">
            <Style
              Name="Toolbar">
              <Properties>
                <Property
                  Name="Display">none</Property>
