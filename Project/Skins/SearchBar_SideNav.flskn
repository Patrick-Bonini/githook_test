<?xml version="1.0" encoding="utf-8"?>
<CatapultSkin
  Version="3"
  SkinType="Component"
  Comment="HTML5 Search Bar skin"
  ComponentType="SearchBar"
  EnableResponsiveOutput="True">
  <Stylesheet>
    <Styles>
      <Style
        Name="Search Submit Button (hover)">
        <Properties>
          <Property
            Name="BackgroundGradient">vertical transparent transparent</Property>
          <Property
            Name="BackgroundImage">url('SearchIconGreen.png')</Property>
          <Property
            Name="BackgroundColor">transparent</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Submit Button">
        <Properties>
          <Property
            Name="Width">40px</Property>
          <Property
            Name="BackgroundGradient">vertical transparent transparent</Property>
          <Property
            Name="BackgroundColor">transparent</Property>
          <Property
            Name="BackgroundImage">url('SearchIconBlack.png')</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Bar">
        <Properties>
          <Property
            Name="Width">100px</Property>
          <Property
            Name="Height">1.9em</Property>
          <Property
            Name="BorderTopLeftRadius">10px</Property>
          <Property
            Name="BorderTopRightRadius">10px</Property>
          <Property
            Name="BorderBottomRightRadius">10px</Property>
          <Property
            Name="BorderBottomLeftRadius">10px</Property>
          <Property
            Name="BorderTop">none 1px #cacacb</Property>
          <Property
            Name="BorderRight">none 1px #cacacb</Property>
          <Property
            Name="BorderBottom">none 1px #cacacb</Property>
          <Property
            Name="BorderLeft">none 1px #cacacb</Property>
        </Properties>
      </Style>
      <Style
        Name="Search Input">
        <Properties>
          <Property
            Name="FontFamily">sans-serif</Property>
          <Property
            Name="FontSize">12px</Property>
        </Properties>
      </Style>
    </Styles>
    <Resources>
      <Resource
        Name="SearchIconBlack.png"></Resource>
      <Resource
        Name="SearchIconGreen.png"></Resource>
    </Resources>
  </Stylesheet>
  <Strings>
    <Stylesheet
      lang="en-us">
      <Stylesheet>
        <Styles>
          <Style
            Name="Search Submit Button (hover)">
            <Properties>
              <Property
                Name="BackgroundColor">#000000</Property>
              <Property
                Name="BackgroundGradient">vertical transparent transparent</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Submit Button">
            <Properties>
              <Property
                Name="Width">40px</Property>
              <Property
                Name="BackgroundGradient">vertical transparent transparent</Property>
              <Property
                Name="BackgroundColor">transparent</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Bar">
            <Properties>
              <Property
                Name="Width">100px</Property>
              <Property
                Name="Height">1.9em</Property>
              <Property
                Name="BorderTopLeftRadius">10px</Property>
              <Property
                Name="BorderTopRightRadius">10px</Property>
              <Property
                Name="BorderBottomRightRadius">10px</Property>
              <Property
                Name="BorderBottomLeftRadius">10px</Property>
              <Property
                Name="BorderTop">none 1px #cacacb</Property>
              <Property
                Name="BorderRight">none 1px #cacacb</Property>
              <Property
                Name="BorderBottom">none 1px #cacacb</Property>
              <Property
                Name="BorderLeft">none 1px #cacacb</Property>
            </Properties>
          </Style>
          <Style
            Name="Search Input">
            <Properties>
              <Property
                Name="Placeholder">Search the docs</Property>
            </Properties>
          </Style>
        </Styles>
      </Stylesheet>
    </Stylesheet>
  </Strings>
</CatapultSkin>